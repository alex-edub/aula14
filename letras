const letra=prompt('Digite a letra:');
if (letra >='a' && letra <='z'){
if (letra ==='a' || letra ==='e' || letra === 'i'|| letra === 'o' ||letra === 'u'){
    console.log ("É uma vogal");
}else{
    console.log("É uma consoante");
}
else{
    console.log("Por favor, digite uma letra válida de 'a' a 'z'.");
